{
    "name": "jagalan/laravel-amazon-advertising-api",
    "description": "Amazon advertising API for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan A. Galan",
            "email": "juan.galan.martinez@gmail.com",
            "homepage": "https://github.com/jagalan"
        }
    ],
    "homepage": "https://github.com/jagalan/laravel-amazona-dvertising-api",
    "keywords": ["Laravel", "Amazon", "Advertising", "API"],
    "require": {
        "illuminate/support": "~5",
        "luchianenco/oauth2-amazon": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jagalan\\AmazonAdvertisingApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jagalan\\AmazonAdvertisingApi\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jagalan\\AmazonAdvertisingApi\\AmazonAdvertisingApiServiceProvider"
            ],
            "aliases": {
                "LaravelAmazonAdvertisingApi": "Jagalan\\AmazonAdvertisingApi\\Facades\\AmazonAdvertisingApi"
            }
        }
    }
}
